{
    "name": "vakata/phptree",
    "description": "A collection of PHP classes for storing a tree in a relational database",
    "keywords": [
        "vakata",
        "tree"
    ],
    "config" : {
        "platform" : {
            "php" : "7.2"
        },
        "optimize-autoloader" : true,
        "sort-packages" : true,
        "bin-compat": "full"
    },
    "homepage": "https://github.com/vakata/phptree",
    "license": "MIT",
    "authors": [
        {
            "name": "vakata",
            "email": "github@vakata.com",
            "homepage": "http://www.vakata.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vakata/phpdoc-md"
        }
    ],
    "require": {
        "vakata/database" : "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "vakata\\phptree\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vakata\\phptree\\test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
